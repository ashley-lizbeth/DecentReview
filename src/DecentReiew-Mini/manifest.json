{
    "manifest_version": 3,
    "name": "DecentReview-Ext",
    "version": "1.0",
    "description": "Consulta el rating de la pagina actual",
    "permissions": ["tabs", "activeTab"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    }
  }